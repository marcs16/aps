<%= render 'layouts/partials/navbar'%>
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <div class="container-fluid">
      <div class="col-md-7">
        <p id="notice"><%= notice %></p>

        <h1>Mantenimientos</h1>

        <table id="maintainces-datatable" data-source="<%= device_maintainces_path(format: :json) %>">
          <thead >
            <tr>
              <th>Realizado por</th>
              <th>Nombre</th>
              <th>Description</th>
              <th>Fecha</th>
              <th>Dispositivo</th>
              <th>Acciones</th>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody>
          </tbody>
        </table>

        <%= link_to 'Agregar mantenimiento', new_device_maintaince_path %>
      </div> 
    </div>
  </section>
  <%=render 'layouts/partials/footer'%>
</div>
