<p id="notice"><%= notice %></p>

<h1>Maintainces</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Date</th>
      <th>User</th>
      <th>Device</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maintainces.each do |maintaince| %>
      <tr>
        <td><%= maintaince.name %></td>
        <td><%= maintaince.description %></td>
        <td><%= maintaince.date %></td>
        <td><%= maintaince.user.full_name %></td>
        <td><%= maintaince.device.name%></td>
        <td><%= link_to 'show', device_maintaince_path(@device,maintaince) %></td>
        <td><%= link_to 'Destroy', device_maintaince_path(@device, maintaince), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Back', devices_path%>
<%= link_to 'New Maintaince', new_device_maintaince_path %>
