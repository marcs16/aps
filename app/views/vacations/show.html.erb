<%=render 'layouts/partials/navbar'%>
<div class="center-style-app">
  <div class="content-wrapper">
    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <h2><%=t('activerecord.models.salary.other')%></h2>
            <p id="notice"><%= notice %></p>
            <div class="card card-primary">
              <div class="card-header">
                <h3><%=t('app_common.shows.showing_for', model: model_name_plural(Vacation)) %> <%= @user_full_name%></h3>
                <div class="card-tools">

                    <button><i class="fa fa-sync-alt"></i></button>
                  <button type="button" class="btn btn-tool btn-sm" data-card-widget="collapse" data-toggle="tooltip"
                          title="Collapse">
                    <i class="fa fa-minus"></i></button>
                </div>
              </div>
              <div class="card-body">
                <form class="form-horizontal">
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.laborated_period')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.laborated_period%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.end_of_laborated_period')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.end_of_laborated_period%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.period_until_deductions')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.period_until_deductions%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.business_days')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.business_days%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.holidays')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.holidays%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.entry_day')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.entry_day%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.exit_day')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.exit_day%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.last_vacations')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.last_vacations%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.acumulated_periods')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.acumulated_periods%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.suspension_days')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.suspension_days%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.date_until_suspension')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.date_until_suspension%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.cost_center')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.cost_center%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.status')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.status%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.observations')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.observations%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class= "col-sm-4 col-form-label">
                      <strong><%= t('activerecord.attributes.vacation.files')%></strong>
                    </div>
                    <div class=" col-sm-7">
                      <div class="form-control col-sm-6">
                        <%= @vacation.files%>
                      </div>
                    </div>
                  </div>
                  <div class="form-group row">
                    <div class="offset-sm-2 col-sm-2">
                      <%= link_to t('helpers.button.back'), :back , class:"btn btn-primary fa fa-arrow-left"%>
                    </div>
                    <div class=" col-sm-1">
                      <%= link_to t('helpers.button.edit'), edit_vacation_path(@vacation), class:"btn btn-primary fa fa-edit" %>
                    </div>
                    <div class="offset-sm-2 col-sm-2">
                      <%=link_to t('activerecord.models.vacation.other'), vacations_path(),class:"btn btn-primary fa fa-cog"%>
                    </div>
                  </div>
                </form>
              </div><!-- /.card-body -->
            </div>
            <!-- /.nav-tabs-custom -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
    <%=render 'layouts/partials/footer'%>
  </div>
</div>
